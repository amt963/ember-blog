<div class="col-md-3">
    <h1>Posts</h1>
    <ul class="list-unstyled">
        {{#each model as |post|}}
            <li>
                {{#link-to "posts.post" post}}
                    {{post.title}}
                <span class="muted">
                    by {{post.author.name}}
                </span>
                {{/link-to}}
            </li>
        {{/each}}
    </ul>
</div>
<div class="col-md-9">
    {{outlet}}
</div>
